---------------------------------------------------------------\r\nconsole.log('**참조형데이터예시**')\r\n\r\nlet a2 = { k: 1};\r\nlet b2 = { k: 1};\r\nconsole.log(a2, b2, a2 === b2); \r\n// 1번 메모리에서 k의값이 변경 (1=>7)\r\na2.k = 7;\r\n// 2번메모리를 바라보던 b2가 1번메모리를 바라보게됨\r\nb2 = a2;\r\nconsole.log(a2, b2, a2 === b2);\r\na2.k = 2;\r\nconsole.log(a2, b2, a2 === b2);\r\nlet c2 = b2;\r\nconsole.log(a2, b2, c2, a2 === c2);\r\na2.k = 9;\r\nconsole.log(a2, b2, c2, a2 === c2)"},"lineCount":null}},"error":null,"hash":"f6e0816f9015a46ec4596ae09d1deca2","cacheData":{"env":{}}}